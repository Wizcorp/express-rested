{
  "name": "express-rested",
  "version": "1.3.2",
  "description": "Manage resources with REST through Express",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint . && jscs -v index.js lib test && tape test/*.js | tap-spec",
    "cover": "rm -rf .nyc_output && nyc --silent --all tape 'test/*.js' > /dev/null 2>&1 && nyc report"
  },
  "keywords": [
    "express",
    "rest",
    "rested",
    "hypermedia",
    "api",
    "resources"
  ],
  "author": "Ron Korving <rkorving@wizcorp.jp>",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.14.1",
    "eslint": "^1.10.3",
    "express": "^4.13.3",
    "jscs": "^2.7.0",
    "nyc": "^4.0.1",
    "pre-commit": "^1.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wizcorp/express-rested.git"
  }
}
